{
    "name": "sapientpro/ebay-browse-sdk-php",
    "description": "PHP SDK for Ebay Browse API",
    "keywords": [
        "php",
        "sdk",
        "api"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "symfony/serializer": "^6.2",
        "symfony/property-access": "^6.2",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.10",
        "symfony/var-dumper": "^6.3",
        "saggre/phpdocumentor-markdown": "^0.1.3"
    },
    "autoload": {
        "psr-4": { "SapientPro\\EbayBrowseSDK\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "SapientPro\\EbayBrowseSDK\\Tests\\": "tests/" }
    }
}
